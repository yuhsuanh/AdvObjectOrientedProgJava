<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.ListPageController">
   <children>
      <TableView fx:id="tableViewCarList" layoutY="115.0" prefHeight="395.0" prefWidth="950.0" />
      <Button fx:id="buttonFilter" layoutX="874.0" layoutY="79.0" mnemonicParsing="false" onAction="#searchCars" text="Search">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="532.0" layoutY="84.0" text="Filter: ">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="checkBoxAll" layoutX="578.0" layoutY="84.0" mnemonicParsing="false" text="All Cars">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="checkBoxSold" layoutX="657.0" layoutY="84.0" mnemonicParsing="false" text="Sold Cars">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="checkBoxAvailable" layoutX="750.0" layoutY="84.0" mnemonicParsing="false" text="Avaliable Cars">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <Button fx:id="buttonCompanyInfo" layoutX="22.0" layoutY="15.0" mnemonicParsing="false" onAction="#enterCompanyInfo" text="Company Info">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="357.0" layoutY="30.0" text="Car Listings">
         <font>
            <Font name="Rockwell Extra Bold" size="33.0" />
         </font>
      </Label>
      <Button fx:id="buttonAddCar" layoutX="22.0" layoutY="79.0" mnemonicParsing="false" onAction="#enterAddCarPage" text="Add Car">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
